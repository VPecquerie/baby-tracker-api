{% block hwi_oauth_content %}
  {% if error and error.messageKey is not defined%}
    <div class="alert alert-primary" role="alert">
      {{ error }}
    </div>
  {% endif %}
  {% for owner in hwi_oauth_resource_owners() %}
    <a class="btn btn-lg btn-primary btn-block" href="{{ hwi_oauth_login_url(owner) }}">
      {{ owner | trans({}, 'HWIOAuthBundle') | capitalize }}
    </a>
  {% endfor %}
{% endblock hwi_oauth_content %}